<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title>Popular Baby Names in the United States, 1880-2014</title>

    <link href='/images/favicon.ico' rel='shortcut icon'/>

    <%= javascript_include_tag "spin" %>
    <%= stylesheet_link_tag "normalize", "all" %>
  </head>

  <body class="<%= page_classes %>" data-spy='scroll' data-target='.scrollspy'>
    <div class='container'>
      <%= yield %>
    </div>
  </body>

  <script>
    window.dataByName = <%= data_by_name %>;
    window.dataByYear = <%= data_by_year %>;
  </script>

  <%= javascript_include_tag  "all" %>
</html>
